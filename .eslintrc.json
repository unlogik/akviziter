{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": [ "eslint:recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly",
        "sap": "readonly",
        "$": "readonly",
        "jQuery": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "no-unused-vars": ["error", {"args":"none"}]
    }
}